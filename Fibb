// fibonacci using for loop
function fibonacci(num) {
    let num1 = 0;
    let num2 = 1;
    let sum;
    
    for (let i = 1; i <= num; i++) {
        console.log(num1)
        sum = num1 + num2;
        num1 = num2;
        num2 = sum;
    }
}

// fibonacci using recursion

function fibonacci(n) {
    if (n == 1)
        return 0;
    if (n == 2)
        return 1;
    return fibonacci(n - 1) + fibonacci(n - 2);
}
